<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="order-management-task-flow-definition">
    <default-activity>getFWMSHoldingRecordsForUserId</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>OrderManagementBean</managed-bean-name>
      <managed-bean-class>com.ciobera.fwms.order.ui.beans.OrderManagementBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="orderManagementPF">
      <page>/com/ciobera/fwms/order/ui/fragments/orderManagementPF.jsff</page>
    </view>
    <method-call id="getFWMSHoldingRecordsForUserId">
      <method>#{bindings.getFWMSHoldingRecordsForUserId.execute}</method>
      <outcome>
        <fixed-outcome>getFWMSHoldingRecordsForUserId</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__2">
      <from-activity-id>getFWMSHoldingRecordsForUserId</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>getFWMSHoldingRecordsForUserId</from-outcome>
        <to-activity-id>orderManagementPF</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
